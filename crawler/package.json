{
  "name": "mud-crawler",
  "version": "1.0.0",
  "description": "AI-powered MUD crawler agent",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/crawler/src/index.js",
    "crawl": "tsx src/index.ts",
    "crawl:document-actions": "tsx src/index.ts -- --task=document-actions",
    "crawl:document-help": "tsx src/index.ts -- --task=document-help",
    "crawl:learn-game": "tsx src/index.ts -- --task=learn-game",
    "crawl:play-game": "tsx src/index.ts -- --task=play-game",
    "learn-commands": "tsx src/commandCrawler.ts",
    "learn-commands:build": "tsc && node dist/commandCrawler.js",
    "discover-races": "tsx src/discoverRaces.ts"
  },
  "dependencies": {
    "telnet-client": "^2.1.0",
    "dotenv": "^16.4.5",
    "axios": "^1.7.7",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
