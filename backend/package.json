{
  "name": "mud-backend",
  "version": "1.0.0",
  "description": "Backend API for MUD crawler data",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "seed": "node seed.js",
    "db:reset": "node -e \"const fs = require('fs'); if(fs.existsSync('mud_data.db')) fs.unlinkSync('mud_data.db'); console.log('Database deleted');\" && npm run seed"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
